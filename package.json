{
    "name": "tunein-app",
    "version": "0.1.1",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "normalize.css": "^8.0.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-router-dom": "^4.2.2",
        "react-scripts": "^1.1.4",
        "stylus": "^0.54.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "deploy": "gh-pages -d build",
        "build-css": "stylus -w src/stylus/app.styl -o src/index.css"
    },
    "homepage": "https://chaozhang.github.io/tunein-app",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "gh-pages": "^1.1.0"
    },
    "babel": {
        "presets": [
            "env",
            "react"
        ],
        "plugins": [
            [
                "transform-object-rest-spread",
                {
                    "useBuiltIns": true
                }
            ]
        ]
    }
}
